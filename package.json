{
  "name": "express-authorshaven",
  "version": "1.0.0",
  "description": "A Social platform for the creative at heart",
  "main": "index.js",
  "scripts": {
    "lint": "eslint ./src",
    "start": "cross-env NODE_ENV=development && babel-node ./src/index.js",
    "dev": "cross-env NODE_ENV=development && nodemon --exec babel-node ./src/index.js",
    "migrations": "npx sequelize-cli db:migrate",
    "undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "undo-seed": "npx sequelize-cli db:seed:undo",
    "test": "cross-env NODE_ENV=test && npm run migrations && nyc mocha --require @babel/register ./tests/*.js  --timeout 25000 --exit && npm run undo",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "db:connect": "babel-node ./src/database/models/ connect"
  },
  "author": "Andela Simulations Programme",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/node": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.6",
    "@babel/register": "^7.8.6",
    "@babel/runtime": "^7.8.4",
    "@hapi/joi": "^17.1.0",
    "@sendgrid/mail": "^6.5.4",
    "@types/hapi__joi": "^16.0.12",
    "babel-eslint": "^10.1.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "cors": "^2.8.4",
    "coveralls": "^3.0.9",
    "cross-env": "^7.0.2",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.15",
    "mailgen": "^2.0.10",
    "make-runnable": "^1.3.6",
    "method-override": "^2.3.10",
    "methods": "^1.1.2",
    "moment": "^2.24.0",
    "sequelize": "^5.21.5"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "babel-plugin-add-module-exports": "^1.0.2",
    "babel-plugin-istanbul": "^6.0.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^7.1.0",
    "mocha-lcov-reporter": "^1.3.0",
    "morgan": "^1.9.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "pg": "^7.18.2",
    "pg-hstore": "^2.3.3",
    "request": "^2.88.2",
    "sequelize-cli": "^5.5.1",
    "swagger-ui-express": "^4.1.3",
    "underscore": "^1.9.2"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  }
}
